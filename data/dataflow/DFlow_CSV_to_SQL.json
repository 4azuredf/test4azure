{
	"name": "DFlow_CSV_to_SQL",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "LinkedCSV",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          order_id as string,",
				"          order_date as string,",
				"          status as string,",
				"          item_id as string,",
				"          sku as string,",
				"          qty_ordered as string,",
				"          price as string,",
				"          value as string,",
				"          discount_amount as string,",
				"          total as string,",
				"          category as string,",
				"          payment_method as string,",
				"          bi_st as string,",
				"          cust_id as string,",
				"          year as string,",
				"          month as string,",
				"          ref_num as string,",
				"          {Name Prefix} as string,",
				"          {First Name} as string,",
				"          {Middle Initial} as string,",
				"          {Last Name} as string,",
				"          Gender as string,",
				"          age as string,",
				"          full_name as string,",
				"          {E Mail} as string,",
				"          {Customer Since} as string,",
				"          SSN as string,",
				"          {Phone No. } as string,",
				"          {Place Name} as string,",
				"          County as string,",
				"          City as string,",
				"          State as string,",
				"          Zip as string,",
				"          Region as string,",
				"          {User Name} as string,",
				"          Discount_Percent as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1"
			]
		}
	}
}